#: ../doc\base\dev\teyvat_move_flow.pygettext:10
msgid "## 参数"
msgstr ""

#: ../doc\base\dev\teyvat_move_flow.pygettext:12
msgid ""
"TeyvatMoveController(MODE:str = None,\n"
"    stop_rule:int = None,\n"
"    target_posi:list = None,\n"
"    path_dict:dict = None,\n"
"    to_next_posi_offset:float = None,\n"
"    special_keys_posi_offset:float = None,\n"
"    reaction_to_enemy:str = None,\n"
"    is_tp:bool=None,\n"
"    precise_arrive:bool=None)"
msgstr ""

#: ../doc\base\dev\teyvat_move_flow.pygettext:16
msgid "AUTO：朝着target_posi直线行走。"
