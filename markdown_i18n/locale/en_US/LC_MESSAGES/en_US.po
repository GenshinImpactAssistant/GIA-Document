msgid "0：到达目标停止。"
msgstr ""

#: ../doc\base\dev\teyvat_move_flow.pygettext:26
msgid "1：识别到F停止。"
msgstr ""

#: ../doc\base\dev\teyvat_move_flow.pygettext:36
msgid ""
